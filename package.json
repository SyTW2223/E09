    "@types/validator": "^13.7.10",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "axios": "^1.0.0",
    "chai": "^4.3.7",
    "core-js": "^3.8.3",
    "cors": "^2.8.5",
    "coveralls": "^3.1.1",
    "env-cmd": "^10.1.0",
    "express": "^4.18.2",
    "jsdom": "^21.0.0",
    "jsonwebtoken": "^8.5.1",
